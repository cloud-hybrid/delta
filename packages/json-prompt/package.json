        "@cloud/http-statuses": "./library/status-codes",
        "@cloud/logging": "./library/logging",
        "@cloud/library": "./library",
        "@cloud/s3": "./library/s3"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/uuid": "^8.3.4",
        "@aws-sdk/types": "^3.47.1",
        "@types/bcrypt": "^5.0.0",
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.13"
    }
